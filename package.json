{
  "name": "repos",
  "version": "1.0.2",
  "description": "Personal CV website and PDF generator",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "build:assets": "node build-minify.js",
    "build:html": "node build-static-locales.js",
    "build:web": "npm run build:assets && npm run build:html",
    "build:pdf": "node build-pdf.js",
    "build": "npm run build:web && npm run build:pdf",
    "pdf": "node generate-pdf.js --vector",
    "pdf:all": "node generate-pdf.js --all --vector",
    "pdf:raster": "node generate-pdf.js --raster",
    "pdf:raster:all": "node generate-pdf.js --all --raster",
    "rehydrate": "node rehydrate-pdf.js",
    "rehydrate:all": "node rehydrate-pdf.js --locale fr --theme dark && node rehydrate-pdf.js --locale fr --theme light && node rehydrate-pdf.js --locale en --theme dark && node rehydrate-pdf.js --locale en --theme light"
  },
  "repository": {
    "type": "git",
    "url": "git@github-etienne:EtienneLescot/cv.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@pdf-lib/fontkit": "^1.1.1",
    "@playwright/test": "^1.58.0",
    "cssnano": "^7.0.0",
    "glob": "^11.0.0",
    "jsdom": "^25.0.1",
    "pdf-lib": "^1.17.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "puppeteer": "^24.10.0",
    "sharp": "^0.34.5",
    "terser": "^5.31.3",
    "yaml": "^2.8.1"
  }
}
